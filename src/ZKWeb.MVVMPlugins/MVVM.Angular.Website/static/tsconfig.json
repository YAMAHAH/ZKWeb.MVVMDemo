{
    "compilerOptions": {
        "declaration": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "target": "es5",
        "module": "es2015",
        "moduleResolution": "node",
        "sourceMap": true,
        "baseUrl": "",
        "typeRoots": [
            "node_modules/@types"
        ],
        "lib": [
            "es2015",
            "dom"
        ],
        "paths": {
            "@base_module/*": [
                "src/modules/base_module/*"
            ],
            "@generated_module/*": [
                "src/modules/generated_module/*"
            ],
            "@global_module/*": [
                "src/modules/global_module/*"
            ],
            "@auth_module/*": [
                "src/modules/auth_module/*"
            ],
            "@admin_modules/*": [
                "src/modules/admin_modules/*"
            ],
            "@application_modules/*": [
                "src/modules/application_modules/*"
            ],
            "@vendor/*": [
                "src/vendor/*"
            ]
        },
        "noEmitHelpers": false
    },
    "include": [
        "./src/**/*.module.ts",
        "./src/*.d.ts"
    ],
    "exclude": [
        "node_modules"
    ],
    "angularCompilerOptions": {
        "genDir": "."
    }
}